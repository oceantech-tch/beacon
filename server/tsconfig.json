{
  "compilerOptions": {
    "target": "ES2020",
    "module": "EsNext",
    "moduleResolution": "bundler",
    "lib": ["ES2022"],
    "rootDir": ".",
    "outDir": "dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
  },
  "include": ["*.ts", "**/*.ts"],
}
